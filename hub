local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ollies script hub", "DarkTheme")

--MAIN
local Main = Window:NewTab("FE")
local MainSection = Main:NewSection("FE")


MainSection:NewButton("Walk on Walls", "Lets you defy gravity.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))()
end)

--ADMIN SECTION
local Admin = Window:NewTab("Admin")
local AdminSection = Admin:NewSection("Admin")


AdminSection:NewButton("Infinite Yield", "omg!1 he has admin!!1.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

AdminSection:NewButton("Nameless Admin", "Dawg why u banging people?????", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FD2Team/Nameless-Admin-No-Byfron-Kick/main/Source",true))()
end)

--LOCAL PLAYER
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("Walkspped", "Changes ur walkspeed :)", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("JumpPower", "Changes ur jumppower :)", 350, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

--Da Hood

local DaHood = Window:NewTab("Da Hood")
local DaHoodSection = DaHood:NewSection("Da Hood")

DaHoodSection:NewButton("L Key Tool", "For the aimlock..", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/imLUDk7h"))()
end)

DaHoodSection:NewButton("Aimlock", "Use L tool to lock, or use L key.", function()
    loadstring(game:HttpGet("https://pastebin.com/JD0HhKYp",true))()
end)

DaHoodSection:NewButton("Zinc Hub", "Has some good money farms.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Zinzs/luascripting/main/canyoutellitsadahoodscriptornot.lua"))()
end)

